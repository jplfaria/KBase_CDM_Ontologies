# Test configuration - high memory usage with comprehensive testing
# Suitable for testing all pipeline components with manageable datasets

# Java memory settings (High settings for test ontologies - same limits as production)
ROBOT_JAVA_ARGS=-Xmx1024g -XX:MaxMetaspaceSize=32g -XX:+UseG1GC
_JAVA_OPTIONS=-Xmx1024g -XX:MaxMetaspaceSize=32g -XX:+UseG1GC
RELATION_GRAPH_JAVA_ARGS=-Xmx512g -XX:MaxMetaspaceSize=16g -XX:+UseG1GC

# Dataset configuration
DATASET_SIZE=test
ONTOLOGIES_SOURCE_FILE=ontologies_source_test.txt

# Test-specific ontologies to merge (all from test set)
ONTOLOGIES_TO_MERGE=bfo,iao,uo,ro,pato,cl,envo,go,taxrank,credit

# Processing options (enable to test full functionality)
ENABLE_REASONING=true
ENABLE_MATERIALIZATION=true
MAX_ONTOLOGY_SIZE_MB=2000

# Output options (test all formats)
OUTPUT_FORMAT=owl,db,tsv
COMPRESS_OUTPUT=false

# Performance tuning (conservative for testing)
PARALLEL_DOWNLOADS=3
BATCH_SIZE=50

# Testing and debugging
VERBOSE_LOGGING=true
MEMORY_LOG_INTERVAL=30
TEST_MODE=true

# Memory monitoring and logging
ENABLE_MEMORY_MONITORING=true
CREATE_UTILS_LOGS=true
MEMORY_MONITOR_INTERVAL=15